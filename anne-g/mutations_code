import random
import string
i = 1

f=open("/share/data/ancestry/asia", "r")
sequence=f.readline()

for i in range(51):
    sequenceLocation = random.randint(1, 1850)
    string.letters = "ATGC"
    newBase = random.choice(string.letters)
    sequence = sequence[:sequenceLocation] + newBase + sequence[sequenceLocation
+1:]

print sequence
